-app/client/src/index.js",[],["143","144"],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/App.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/reportWebVitals.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/hooks-store/stores/user-credential-store.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/hooks-store/store.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/LandingPage/LandingPage.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/SignupForm/SignupForm.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/ErrorPage/ErrorPage.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/Genres/Genres.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/constants/genres.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/hooks-store/stores/filters-store.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/SetFilters/SetFilters.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/containers/MovieSwap/MovieSwap.js",["145","146","147","148","149"],"import SetFilters from '../../components/SetFilters/SetFilters';\nimport { useStore } from '../../hooks-store/store';\nimport './MovieSwap.css';\nimport RandomMovie from '../../components/RandomMovie/RandomMovie';\nimport Controlls from '../../components/Controlls/Controlls';\nimport { fetchAllMovies, fetchAllGenres } from '../../utils/fetchMovies';\nimport { chooseMovie } from '../../utils/chooseMovie';\nimport { useEffect, useState } from 'react';\nimport { fetchUserMovies } from '../../utils/fetchUserMovies';\nimport Friends from '../../components/Friends/Friends';\nimport SyncedFriends from '../../components/Friends/SyncedFriends/SyncedFriends';\nimport LikesSeenDislikedMovies from '../../components/LikesSeenDislikedMovies/LikesSeenDislikedMovies';\n\nconst MovieSwap = () => {\n    const [globalState, dispatch] = useStore();\n    const [movies, setMovies] = useState([]);\n    const [curMovie, setCurMovie] = useState(null);\n    const [fetchPages, setFetchPages] = useState(10);\n    const [allGenres, setAllGenres] = useState([]);\n\n    useEffect(() => {\n        const userMovies = { ...globalState.movies };\n        if (movies.length && Object.keys(userMovies).length) {\n            const filters = { years: globalState.years, scores: globalState.scores, genres: globalState.genres };\n            const movie = chooseMovie(movies, allGenres, userMovies.seenMovies, userMovies.likedMovies, userMovies.dislikedMovies, filters);\n            if (!movie) {\n                return setFetchPages(oldState => oldState + 10); // fetch every 10 pages. e.g., 0-10, 10-20, 20-30 etc\n            }\n            // TODO: get IMDB rating\n            setCurMovie(movie);\n        } else {\n            setCurMovie(null);\n        }\n    }, [\n        movies,\n        allGenres,\n        globalState.movies && globalState.movies.dislikedMovies,\n        globalState.movies && globalState.movies.likedMovies,\n        globalState.movies && globalState.movies.seenMovies\n    ]);\n\n    useEffect(() => {\n        fetchAllGenres(data => setAllGenres(data));\n    }, []);\n\n    useEffect(() => {\n        fetchAllMovies(data => setMovies(data), fetchPages);\n    }, [fetchPages]);\n\n    useEffect(() => {\n        fetchUserMovies(globalState.userId, data => dispatch('SET_USER_MOVIES', data)); // {likedMoves, seenMovies, DislikedMovies}\n    }, [])\n\n    return (\n        <div className=\"container\">\n            <div className=\"header\">\n                <div className=\"logo\">Logo</div>\n                <div className=\"logout-button\">Logout</div>\n            </div>\n            <div className=\"main\">\n                <div className=\"filters-main\">\n                    <SetFilters />\n                </div>\n                <div className=\"movie-main\">\n                    {curMovie ? <RandomMovie movie={curMovie} /> : <h3>No movies, please change filters. <button onClick={() => setFetchPages(10)}>ℛ</button></h3>}\n                    {curMovie && <Controlls movieId={curMovie.id} userId={globalState.userId} />}\n                </div>\n                <div className=\"friends-main\">\n                    <Friends />\n                </div>\n            </div>\n            <div className=\"footer\">\n                <LikesSeenDislikedMovies />\n                <SyncedFriends />\n            </div>\n        </div>\n    );\n};\n\nexport default MovieSwap;","/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/RandomMovie/RandomMovie.js",["150"],"import { useEffect, useState } from 'react';\nimport './RandomMovie.css';\nimport { fetchMovieDetails, fetchMoveCredits } from '../../utils/fetch-movie-details';\nimport { getDirectors, getStars, getWriters } from '../../utils/extractCredits';\n\nconst RandomMovie = ({ movie }) => {\n    const [mvDetails, setMvDetails] = useState({});\n    const [mvCredits, setMvCredits] = useState({});\n\n    useEffect(() => {\n        fetchMovieDetails(movie.id, data => setMvDetails(data));\n        fetchMoveCredits(movie.id, data => setMvCredits(data));\n    }, [movie, fetchMovieDetails]);\n\n    let mvGenres;\n    if (Object.keys(mvDetails).length) {\n        mvGenres = mvDetails.genres.reduce((acc, genre) => acc + genre.name + ' ', '')\n    }\n\n    const credits = {};\n    if (Object.keys(mvCredits).length) {\n        credits['stars'] = getStars(mvCredits.cast);\n        credits['writers'] = getWriters(mvCredits.crew);\n        credits['directors'] = getDirectors(mvCredits.crew);\n    }\n\n    return (\n        <div className=\"mv-item\">\n            <div className=\"mv-item-inner\">\n                <h2 className=\"mv-title-main\">{movie.original_title} <span>({movie.release_date.slice(0, 4)})</span></h2>\n                <span className=\"mv-duration-main\">{mvDetails.runtime}m&nbsp;|</span>\n                <span className=\"mv-genre-main\">{mvGenres}</span>\n                <span className=\"mv-score-main\">|&nbsp;{mvDetails.vote_average}★</span>\n                <img className=\"mv-img-main\" src={`https://image.tmdb.org/t/p/original/${movie.poster_path}`} alt=\"\" />\n                <span className=\"mv-details-main\"> <hr/> {mvDetails.overview}</span>\n                <span className=\"mv-director-main\"> <hr/> <b>Director</b>(s): {credits.directors}</span>\n                <span className=\"mv-writer-main\"><b>Writer</b>(s): {credits.writers}</span>\n                <span className=\"mv-stars-main\"><b>Starts</b>: {credits.stars}</span>\n            </div>\n        </div>\n    );\n};\n\nexport default RandomMovie;","/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/Controlls/Controlls.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/utils/fetchMovies.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/constants/TMDbApi.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/utils/chooseMovie.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/utils/fetchUserMovies.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/hooks-store/stores/movies-store.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/utils/add-movies-backend.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/Login/Login.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/ListMovies/ListMovies.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/utils/remove-movies-backend.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/Friends/Friends.js",["151"],"import { useEffect, useState } from 'react';\nimport './Friends.css';\nimport { fetchFriends, removeFriend, fetchFriendWithMovies } from '../../utils/friends';\nimport { useStore } from '../../hooks-store/store';\nimport AddFriend from './AddFriend/AddFriend';\n\nconst Friends = () => {\n    const [globalState, dispatch] = useStore();\n    const [friends, setFriends] = useState([]);\n\n    useEffect(() => {\n        fetchFriends(globalState.userId, data => setFriends(data.friends));\n    }, []);\n\n    const removefriendFromListHandler = friendId => {\n        removeFriend(friendId, globalState.userId, data => setFriends(data.friends));\n    };\n\n    const toggleSyncWithFriendHandler = friendId => {\n        const updatedSyncedFriends = globalState.syncedFriends.filter(friend => friend.id !== friendId);\n        // if there was the friend already synced\n        if (updatedSyncedFriends.length < globalState.syncedFriends.length) {\n            return dispatch('SET_SYNCED_FRIENDS', updatedSyncedFriends);\n        }\n        // else add them\n        fetchFriendWithMovies(globalState.userId, friendId, data => {\n            updatedSyncedFriends.push(data.friend);\n            dispatch('SET_SYNCED_FRIENDS', updatedSyncedFriends);\n        });\n    };\n\n    let friendsList = <h5>You have no friends :( Add a friend</h5>;\n    if (friends.length) {\n        friendsList = friends.map(friend => {\n            return (\n                <li key={friend.email}>\n                    {friend.name}\n                    <div\n                        onClick={() => toggleSyncWithFriendHandler(friend.id)}\n                        style={{\n                            display: 'inline-block',\n                            color: globalState.syncedFriends.some(syncedFriend => syncedFriend.id === friend.id) ? 'green' : 'gray',\n                            cursor: 'pointer'\n                        }}>\n                        &nbsp;♼\n                </div>\n                    <div\n                        onClick={() => removefriendFromListHandler(friend.id)}\n                        style={{ display: 'inline-block', color: 'red', cursor: 'pointer' }}>\n                        &nbsp;x\n                </div>\n                </li>\n            );\n        });\n    }\n\n\n    return (\n        <div className=\"friends-container\">\n            <div className=\"friends-list-container\">\n                <h3>Friends <hr /></h3>\n                <ul className=\"friends-list\">\n                    {friendsList}\n                </ul>\n            </div>\n            <AddFriend userId={globalState.userId} onAddFriend={setFriends} />\n        </div>\n    );\n};\n\nexport default Friends;","/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/utils/friends.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/Friends/AddFriend/AddFriend.js",[],"/Users/nikolastheodoropoulos/Documents/CodeClan_Software-dev/codeclan_work/movies-swap-app/client/src/components/Friends/SyncedMovieList/SyncedMovieList.js",["152"],"import { useEffect, useState } from 'react'